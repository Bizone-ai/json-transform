{
  "finds the first truthy element of the input array, using inline definition (inline form)": "$$find(##current):$",
  "finds the first element of the input array that is truthy (object form)": {
    "$$find": "$",
    "by": "##current"
  },
  "finds the first element of the input array that is explicitly boolean (object form)": {
    "$$find": "$",
    "by": "$$boolean:##current"
  },
  "finds the first element of the input array whose name starts with 'b' (object form)": {
    "$$find": "$",
    "by": "$$test('(?i)^b'):##current.name"
  }
}