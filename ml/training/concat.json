{
  "concatenates two arrays from the input object, one in a property \"a\" and the other in property \"b\" (object form)": {
    "$$concat": [
      "$.a",
      "$.b"
    ]
  },
  "concatenates the input array with a single string \"d\" (object form)": {
    "$$concat": [
      "$",
      "d"
    ]
  },
  "concatenates the input array with a static array of 2 strings \"d\" and \"e\" (object form)": {
    "$$concat": [
      "$",
      [
        "d",
        "e"
      ]
    ]
  },
  "blindly concatenates the input (object form)": {
    "$$concat": "$"
  },
  "blindly concatenates the input (inline form)": "$$concat:$"
}